	<queryString>
		<![CDATA[select iif(tm.direcao='E',sum(tc.entrada),sum(tc.saida)) as valorsub,
        tm.descricao_movimento as subtipo,
        tm.direcao,tc.id_conta as idconta
 from tmov_conta tc
